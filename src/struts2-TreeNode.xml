<struts>
	<package name="treenode" namespace="/treeNode" extends="struts-default">
		<action name="*json" class="com.wjl.action.TreeNodeAction" method="{1}Node">
			<result>/index.jsp</result>
		</action>
		<action name="confirm" class="com.wjl.action.ConfirmAction" method="confirm">
			<result></result>
		</action>
	</package>
</struts>    
